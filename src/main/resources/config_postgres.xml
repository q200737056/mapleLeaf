<?xml version="1.0" encoding="UTF-8"?>
<configure>
	<global>
		<!--生成代码路径 ,默认项目所在目录-->
		<property name="baseDir" value="f:\\code" />
		<!--基础包名，默认mapleLeaf.code-->
		<property name="basePackage" value="com.test" />
		<!-- 是否去表名前缀 ,默认false-->
		<property name="deleteTabPrefix" value="false" />
		<!-- 全局表名前缀，多个逗号分隔 ,默认""-->
		<property name="baseTabPrefix" value="" />
		<!-- 表字段是否下划线转驼峰命名 ，默认false-->
		<property name="columnCamel" value="false" />

		<property name="persistance" value="mybatis" />
		
	</global>
	<db>
		<property name="dbType" value="postgresql"/>
		<property name="dbName" value="test"/>
		<property name="user" value="postgres"/>
		<property name="pwd" value="q123"/>
		<property name="driver" value="org.postgresql.Driver"/>
		<property name="url" value="jdbc:postgresql://localhost:5432/test"/>
	</db>
	
	<module>
		<entityPackage>entity</entityPackage>
		<daoPackage>dao</daoPackage>
		<servicePackage>service</servicePackage>
		<controllerPackage>controller</controllerPackage>
		<mapperPackage>mapper.xml</mapperPackage>
		<viewPackage tpl="Jsp_list" suffix="html"></viewPackage>
		<customPackage tpl="Jsp_add" suffix="java"></customPackage>
		
		<table name="sys_user" prefix=""></table>
	</module>
</configure>