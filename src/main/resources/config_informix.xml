<?xml version="1.0" encoding="UTF-8"?>
<!-- 使用前，请好好查看配置文件说明及使用说明-->
<configure>
	<global>
		<!--生成代码路径 ,默认项目所在目录-->
		<property name="baseDir" value="f:\\code" />
		<!--基础包名，默认mapleLeaf.code-->
		<property name="basePackage" value="com.test" />
		<!-- 是否去表名前缀 ,默认false-->
		<property name="deleteTabPrefix" value="false" />
		<!-- 全局表名前缀，多个逗号分隔 ,默认""-->
		<property name="baseTabPrefix" value="sys_,wxcms_" />
		<!-- 表字段是否下划线转驼峰命名 ，默认false-->
		<property name="columnCamel" value="false" />
		<property name="persistance" value="mybatis" />
		
	</global>
	
	<db>
		<property name="dbType" value="informix"/>
		<property name="dbName" value="rmbp"/>
		<property name="user" value="xxxx"/>
		<property name="pwd" value="xxxx"/>
		<property name="driver" value="com.informix.jdbc.IfxDriver"/>
		<property name="url" 
		value="jdbc:informix-sqli://XXXXX/rmbp:informixserver=zjtsywtsdb;DB_LOCALE=en_us.819;CLIENT_LOCALE=en_us.57372;NEWCODESET=GBK,8859-1,819"/>
		
	</db>
	
	<module>
		<entityPackage>entity</entityPackage>
		<daoPackage>dao</daoPackage>
		<servicePackage>service</servicePackage>
		<controllerPackage>controller</controllerPackage>
		<!-- hibernate不需要，可以设置空 -->
		<mapperPackage></mapperPackage>
		<viewPackage tpl="Jsp_list" suffix="html">jsp</viewPackage>
		<!-- 自定义配置，模板自定，比如js文件等等 -->
		<customPackage tpl="Jsp_add" suffix="java">custom</customPackage>
		
		<table name="authorize_param" prefix="">
		</table>
		
	</module>
</configure>